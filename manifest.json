{
  "manifest_version": 3,
  "name": "Đức Chung",
  "version": "1.0",
  "description": "Tự động trích xuất dữ liệu từ trang web và tải về",
  "permissions": ["tabs", "scripting", "storage", "activeTab", "downloads"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup/UI/popup.html",
    "default_icon": "access/banhTrung.png"
  },
  "background": {
    "service_worker": "backgrounds/background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
